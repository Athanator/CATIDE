SELECT      

ROWNUM,
a.ac_registr as REG,
/********************************************************************************************* Nº REFERENCIA**************************************************************************/
CASE 
WHEN ROWNUM IN(6,	9,	12,	14,	19,	37,	39,	44,	48,	50,	54,	57,	59,	61,	63,	78,	89,	96,	101,	111,	122,		136,		150,	154,		165,	176,	186,	189,	194,	201,	203,	207,	216
)  THEN NULL
WHEN ROWNUM = 1 THEN 1
WHEN ROWNUM = 7 THEN 2
WHEN ROWNUM = 10 THEN 3
WHEN ROWNUM = 13 THEN 4
WHEN ROWNUM = 15 THEN 5
WHEN ROWNUM = 20 THEN 6
WHEN ROWNUM = 38 THEN 7
WHEN ROWNUM = 40 THEN 8
WHEN ROWNUM = 45 THEN 9
WHEN ROWNUM = 49 THEN 10
