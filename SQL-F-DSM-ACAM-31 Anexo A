                                 AND wo_event_link.event_status IN(
                                    'C',
                                    'V',
                                    'F'                                  
                                 )                         
                        ) wel2 ON dh2.docno_i = wel2.event_key_parent                      
                   LEFT JOIN wo_header wh2 ON wel2.event_perfno_i = wh2.event_perfno_i                      
                   WHERE dh2.docno = dh.docno                           
                         AND wh2.ac_registr = wh.ac_registr                   
                )               
             ) OVER(
                PARTITION BY wh.ac_registr               
             ORDER BY dh.rev_date desc    
   )
   FROM doc_header dh
   INNER JOIN (
      SELECT DISTINCT wo_event_link.event_key_parent,
             wo_event_link.event_perfno_i,
             wo_event_link.event_status        
      FROM wo_event_link        
      WHERE wo_event_link.pending_status = 0              
            AND wo_event_link.event_type = 'MED'    
   ) wel ON dh.docno_i = wel.event_key_parent
   LEFT JOIN wo_header wh ON wel.event_perfno_i = wh.event_perfno_i
   INNER JOIN doc_header dh3 ON 
   (
      SELECT DISTINCT dh2.docno        
      FROM (
              SELECT DISTINCT doc_header.docno,
                     doc_header.docno_i,
                     MAX(doc_header.rev_date) OVER(PARTITION BY wh.ac_registr)                
              FROM doc_header                
              WHERE doc_header.docno = dh.docno             
           ) dh2  INNER        
      JOIN (
              SELECT DISTINCT wo_event_link.event_key_parent,
                     wo_event_link.event_perfno_i,
                     wo_event_link.event_status                
              FROM wo_event_link                
              WHERE wo_event_link.pending_status = 0                      
                    AND wo_event_link.event_type = 'MED'                      
                    AND wo_event_link.event_status IN(
                       'C',
                       'V',
                       'F'                     
                    )            
           ) wel2 ON dh2.docno_i = wel2.event_key_parent         
      LEFT JOIN wo_header wh2 ON wel2.event_perfno_i = wh2.event_perfno_i         
      WHERE dh2.docno = dh.docno              
            AND wh2.ac_registr = wh.ac_registr      
   ) = dh3.docno
   WHERE dh.docno IN(
      '27620',
      '34429',
      '35536',
      '39475',
      '34-1335'
      ) AND wh.ac_registr = a.ac_registr
   )  IS NOT NULL THEN (
   SELECT DISTINCT MAX(
             (
                SELECT DISTINCT dh2.docno                  
                FROM (
                        SELECT DISTINCT doc_header.docno,
                               doc_header.docno_i,
                               MAX(doc_header.rev_date) OVER(PARTITION BY wh.ac_registr)                          
                        FROM doc_header                          
                        WHERE doc_header.docno = dh.docno                       
                     ) dh2  INNER                  
                JOIN (
                        SELECT DISTINCT wo_event_link.event_key_parent,
                               wo_event_link.event_perfno_i,
                               wo_event_link.event_status                          
                        FROM wo_event_link                          
                        WHERE wo_event_link.pending_status = 0                                
                              AND wo_event_link.event_type = 'MED'                                
                              AND wo_event_link.event_status IN(
                                 'C',
                                 'V',
                                 'F'                               
                              )                      
                     ) wel2 ON dh2.docno_i = wel2.event_key_parent                   
                LEFT JOIN wo_header wh2 ON wel2.event_perfno_i = wh2.event_perfno_i                   
                WHERE dh2.docno = dh.docno                        
                      AND wh2.ac_registr = wh.ac_registr                
             )            
          ) OVER(
             PARTITION BY wh.ac_registr            
          ORDER BY dh.rev_date desc 
)
FROM doc_header dh
INNER JOIN (
   SELECT DISTINCT wo_event_link.event_key_parent,
          wo_event_link.event_perfno_i,
          wo_event_link.event_status     
   FROM wo_event_link     
   WHERE wo_event_link.pending_status = 0           
         AND wo_event_link.event_type = 'MED' 
) wel ON dh.docno_i = wel.event_key_parent
LEFT JOIN wo_header wh ON wel.event_perfno_i = wh.event_perfno_i
INNER JOIN doc_header dh3 ON 
(
   SELECT DISTINCT dh2.docno     
   FROM (
           SELECT DISTINCT doc_header.docno,
                  doc_header.docno_i,
                  MAX(doc_header.rev_date) OVER(PARTITION BY wh.ac_registr)             
           FROM doc_header             
           WHERE doc_header.docno = dh.docno          
        ) dh2  INNER     
   JOIN (
           SELECT DISTINCT wo_event_link.event_key_parent,
                  wo_event_link.event_perfno_i,
                  wo_event_link.event_status             
           FROM wo_event_link             
           WHERE wo_event_link.pending_status = 0                   
                 AND wo_event_link.event_type = 'MED'                   
                 AND wo_event_link.event_status IN(
                    'C',
                    'V',
                    'F'                  
                 )         
        ) wel2 ON dh2.docno_i = wel2.event_key_parent      
   LEFT JOIN wo_header wh2 ON wel2.event_perfno_i = wh2.event_perfno_i      
   WHERE dh2.docno = dh.docno           
         AND wh2.ac_registr = wh.ac_registr   
) = dh3.docno
WHERE dh.docno IN(
   '27620',
   '34429',
   '35536',
   '39475',
   '34-1335'
   ) AND wh.ac_registr = a.ac_registr
)   ELSE 'NOT INSTALLED'
END
)  || ' (Instalación del Integrated Standby Instrument (ISIS)'
